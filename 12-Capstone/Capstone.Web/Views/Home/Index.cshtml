@model IList<Park>
@{
    ViewData["Title"] = "Home Page";
}


<h1>National Park</h1>
<h1>Geek</h1>


<table class="table table-bordered table-striped">
    @foreach (Park park in Model)
    {
    <tr>
        <td><a asp-controller="home" asp-action="detail" asp-route-parkcode="@park.ParkCode"><img src="~/images/parks/@(park.ParkCode).jpg" alt=" Scenic View of Park" /></a></td>
        <td><h4><strong>@park.ParkName</strong></h4>
        <p>@park.ParkDescription</p> </td>
    </tr>
    }
</table>
