@model IList<Park>
@{
    ViewData["Title"] = "Home Page";
}

<table id="IndexTable" class="table table-bordered">
    @foreach (Park park in Model)
    {
    <tr>
        <td><a asp-controller="home" asp-action="detail" asp-route-parkcode="@park.ParkCode"><img src="~/images/parks/@(park.ParkCode).jpg" alt=" Scenic View of Park" /></a></td>
        <td>
            <a asp-controller="home" asp-action="detail" asp-route-parkcode="@park.ParkCode">
                <h4><strong>@park.ParkName</strong></h4></a>
                <h6><strong>@park.State</strong></h6>
                <p>@park.ParkDescription</p>

</td>
    </tr>
    }
</table>
